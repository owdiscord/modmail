{
	"name": "modmailbot",
	"version": "4.0.0",
	"description": "",
	"license": "MIT",
	"main": "src/index.js",
	"type": "module",
	"scripts": {
		"start": "bun src/index.js",
		"dev": "NODE_ENV=development bun src/index.js",
		"web": "bun --watch src/web.tsx",
		"watch": "NODE_ENV=development bun --watch src/index.js",
		"lint": "biome check ./src",
		"lint-fix": "biome check --write --unsafe ./src",
		"generate-config-jsdoc": "bun src/data/generateCfgJsdoc.js",
		"generate-plugin-api-docs": "jsdoc2md -t docs/plugin-api-template.hbs src/pluginApi.js > docs/plugin-api.md",
		"migrate:up": "bun ./src/migrate.ts up",
		"migrate:new": "bun ./src/migrate.ts new",
		"migrate:down": "bun ./src/migrate.ts down"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ow2discord/modmail"
	},
	"dependencies": {
		"ajv": "^8.17.1",
		"discord-api-types": "^0.37.101",
		"discord.js": "^14.25.1",
		"dotenv": "^16.4.5",
		"hono": "^4.11.3",
		"humanize-duration": "^3.32.1",
		"ini": "^4.1.3",
		"json5": "^2.2.3",
		"knub-command-manager": "^6.1.0",
		"marked": "^17.0.1",
		"mysql2": "^3.11.3",
		"public-ip": "^6.0.2",
		"uuid": "13.0.0",
		"yargs-parser": "^21.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "^1.3.5",
		"@types/humanize-duration": "^3.27.4",
		"@types/ini": "^4.1.1",
		"@types/yargs-parser": "^21.0.3",
		"jsdoc-to-markdown": "^8.0.3",
		"json-schema-to-jsdoc": "^1.1.1"
	},
	"engines": {
		"bun": ">=1.3.0"
	}
}
